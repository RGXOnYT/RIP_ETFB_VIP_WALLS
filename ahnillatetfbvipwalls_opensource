-- [[ wall pathz ]] --
local pathsToCheck = {
    {"DefaultMap_SharedInstances", "VIPWalls", "Shared Map"},
    {"ArcadeMap_SharedInstances", "VIPWalls", "Arcade Map"},
    {"UFOMap_SharedInstances", "VIPWalls", "UFO Map"},
    {"MoneyMap_SharedInstances", "VIPWalls", "Money Map"},
    {"NukeMap_SharedInstances", "VIPWalls", "Nuke Map"}
}

local function findAndDestroy(pathData)
    local locations = {workspace, game:GetService("ReplicatedStorage")}
    local mapName = pathData[#pathData]
    local found = false

    for _, root in ipairs(locations) do
        local current = root
        for i = 1, #pathData - 1 do
            if current then
                current = current:FindFirstChild(pathData[i])
            else
                break
            end
        end

        if current then
            found = true
            if current:IsA("BasePart") then
                current:Destroy()
            else
                for _, obj in ipairs(current:GetDescendants()) do
                    if obj:IsA("BasePart") then obj:Destroy() end
                end
            end
            print("✅ GOOD NEWS " .. mapName .. " VIP Walls DELETED.")
            break 
        end
    end
end

DeleteButton.MouseButton1Click:Connect(function()
    DeleteButton.Text = "WIPING..."
    DeleteButton.BackgroundColor3 = Color3.fromRGB(100, 0, 0)
    for _, path in ipairs(pathsToCheck) do
        findAndDestroy(path)
    end
    DeleteButton.Text = "wallz ahnillated"
    task.wait(1.5)
    DeleteButton.Text = "ahnillate vip wallz"
    DeleteButton.BackgroundColor3 = Color3.fromRGB(40, 0, 0)
end)

-- [[ resize logic (w feature) ]] --
local userInputService = game:GetService("UserInputService")
local draggingResize = false

ResizeHandle.MouseButton1Down:Connect(function() draggingResize = true end)
userInputService.InputChanged:Connect(function(input)
    if draggingResize and input.UserInputType == Enum.UserInputType.MouseMovement then
        local mousePos = userInputService:GetMouseLocation()
        local framePos = MainFrame.AbsolutePosition
        local newSize = UDim2.new(0, math.max(280, mousePos.X - framePos.X), 0, math.max(110, mousePos.Y - framePos.Y - 36))
        MainFrame.Size = newSize
    end
end)
userInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then draggingResize = false end
end)
